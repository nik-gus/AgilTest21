pipeline {
    agent { 
    label 'mac'
    }
        
    tools {
        maven 'M3'
    }
    
    options { timestamps () }
    
    stages {
        stage ('Build') {
            steps {
                withEnv(['PATH+EXTRA=/usr/sbin:/usr/bin:/sbin:/bin']) {  
                    sh '''
                        mvn clean install
                        mvn clean install -DWebDriver=Firefox
                    '''
                }
            }
        }
    }
    }
     
 
